; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="LightDark"
run/main_scene="res://Scenes/node_2d.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
run/max_fps=30
config/icon="res://Images/icon.svg"

[display]

window/size/viewport_width=1200
window/size/viewport_height=800
window/size/window_width_override=2400
window/size/window_height_override=1600
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://Images/cursor.png"
mouse_cursor/tooltip_position_offset=Vector2(2.08165e-12, 2.08165e-12)

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/canvas_textures/default_texture_repeat=1
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/default_filters/use_nearest_mipmap_filter=true
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
